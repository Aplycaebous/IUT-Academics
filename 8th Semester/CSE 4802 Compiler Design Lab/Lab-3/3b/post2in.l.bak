%option noyywrap

%{

#include <stdio.h>
#include "y.tab.h"
#undef yylval
char *yylval;	//Defining yylval as a string 
%}

%%
[\t ]+       ;
[a-z]     {
            yylval = yytext[0];	//Only alphabets are allowed as identifiers
            return(LETTER);
          }
[0-9]+     {
	    yylval = yytext;	//The numbers are stored as strings
            return(NUMBER);
          }

[^a-z0-9\b]    {
                 return(yytext[0]);
               }

%%